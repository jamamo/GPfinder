# Copy this file to .env and fill in real values before deploying

# Required: random secret key for signing session cookies
# Generate one with: py -3 -c "import secrets; print(secrets.token_hex(32))"
SECRET_KEY=change-me-to-a-random-string

# Required: admin panel password (change from default)
ADMIN_PASSWORD=change-me-to-a-strong-password

# Set to 'true' in production to enforce HTTPS and secure cookies
FORCE_HTTPS=false
